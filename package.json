{
  "name": "ts-template",
  "version": "1.0.0",
  "description": "Template for projects built with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/index.js",
    "dev": "nodemon src/index.ts",
